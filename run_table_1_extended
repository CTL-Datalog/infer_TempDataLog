### Function 
./main.exe tests/ctl/report/test_until.c -minimal -time -domain polyhedra -joinbwd 2 -precondition "true" -ctl "AU{x >= y}{x == y}"

./main.exe tests/ctl/report/test_existential3.c -minimal -time -domain polyhedra -joinbwd 2 -ctl_existential_equivalence -precondition "x > 0" -ctl "EF{r==1}"

./main.exe tests/ctl/global_test_simple.c -minimal -time -domain polyhedra -joinbwd 6 -precondition "true" -ctl "AG{AF{x <= -10}} "

./main.exe tests/countdown.c -minimal -time -domain polyhedra -joinbwd 2 -precondition "true" -ctl "AF{x == 0} "

./main.exe tests/guarantee/mnav.c -minimal -time -domain polyhedra -joinbwd 2 -precondition "true" -ctl "AF{enable == 0} "

